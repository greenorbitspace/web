---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import HeroSection from '../components/HeroSection.astro';
import StripeWrapper from '../components/StripeWrapper.jsx';
---

<Layout title="Checkout">
  <Header />

  <main class="py-8">
    <HeroSection 
      title="Experience" 
      description="Your privacy matters. Here's how we handle your data at Green Orbit Digital."
    />

<section class="py-12 bg-white dark:bg-secondary-500">

  <StripeWrapper client:load />

    <section/>
  </main>

  <Footer />
</Layout>